{
		"name": "jupyterlab-emacskeys",
		"version": "0.5.0",
		"description": "Emacs keybindings inside the notebook cells of JupyterLab.",
		"author": "kpe",
		"main": "lib/index.js",
		"types": "lib/index.d.ts",
		"license": "MIT",
		"homepage": "https://github.com/kpe/jupyterlab-emacskeys",
		"bugs": {
				"url": "https://github.com/kpe/jupyterlab-emacskeys/issues"
		},
		"repository": {
				"type": "git",
				"url": "https://github.com/kpe/jupyterlab-emacskeys.git"
		},
		"keywords": [
				"extension",
				"jupyter",
				"jupyterlab",
				"jupyterlab-extension",
				"emacs-keybindings"
		],
		"scripts": {
				"build": "tsc",
				"clean": "rimraf lib",
				"prepare": "npm run build",
				"watch": "tsc -w"
		},
		"files": [
				"lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
				"style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
		],
		"jupyterlab": {
				"extension": true
		},
		"dependencies": {
				"@jupyterlab/application": "^1.0.1",
				"@jupyterlab/codemirror": "^1.0.1",
				"@jupyterlab/cells": "^1.0.1",
				"@jupyterlab/notebook": "^1.0.1",
				"@phosphor/commands": "^1.6.3",
				"@phosphor/coreutils": "^1.3.1",
				"@phosphor/domutils": "^1.1.3",
				"@types/codemirror": "^0.0.76"
		},
		"devDependencies": {
				"rimraf": "^2.6.2",
				"typescript": "~3.5.1"
		}
}
