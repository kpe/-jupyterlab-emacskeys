{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES6",
    "outDir": "./lib",
    "lib":["ES6", "DOM"],
    "jsx":"react"
  },
  "include": ["src/*"]
}
